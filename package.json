{
  "name": "triplemint-listing",
  "version": "0.0.0",
  "description": "Triplemint interview assignment. A recreation of the listing page at https://www.triplemint.com/listings?listing_type=sale using svelte.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arschmitz/triplemint-listing.git"
  },
  "author": "Alexander Schmitz",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/arschmitz/triplemint-listing/issues"
  },
  "homepage": "https://github.com/arschmitz/triplemint-listing#readme",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "standard --fix src/*.js",
    "lint-svelte": "standard --plugin html 'src/*.svelte'",
    "start": "sirv public",
    "test": "jest src"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/svelte": "^3.0.0",
    "babel-jest": "^26.3.0",
    "jest": "^26.4.2",
    "jest-transform-svelte": "^2.1.1",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "standard": "^14.3.4",
    "svelte": "^3.0.0",
    "svelte-jester": "^1.1.5"
  },
  "dependencies": {
    "eslint-plugin-html": "^6.1.0",
    "sirv-cli": "^1.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.svelte$": "svelte-jester",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}
